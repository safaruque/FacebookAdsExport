{
    "name": "faruque/facebook-ads-export",
    "description": "A PHP Library for exporting Facebook Advertising Insights",
    "keywords": [
      "facebook", "ads", "export"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Safique Ahmed Faruque",
            "email": "safaruque1@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "facebook/php-sdk-v4" : "~5.0",
        "facebook/php-ads-sdk": "2.7.*",
        "vlucas/phpdotenv": "^2.0"
    },
    "require-dev": {
        "phpunit/phpunit" : "4.*"
    },
    "autoload": {
        "psr-4": {"FacebookAdsExporter\\": "src/"}
    },
    "autoload-dev": {
        "psr-4": {
            "FacebookAdsExporter\\Test\\": "tests"
        }
    }
}
